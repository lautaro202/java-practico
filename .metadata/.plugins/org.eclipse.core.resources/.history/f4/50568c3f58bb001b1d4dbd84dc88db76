
public class Persona {
	public Persona() {}
		private String nombre;
		private short edad;
		private int DNI;
		
		public void setName(String name) {
			nombre = name;
		}
		
		public String getName() {
			return nombre;
		}
		
		public void setAge(short age) {
			edad = age;
		}
		public short getAge() {
			return edad;
		}
		public void setDNI(int identity) {
			DNI = identity;
		}
		
}
